def expand(op: CommandOp): CommandOp = op match {
  case `/dev/fd`(op0@_) => expand(op0) 
  case `/dev/tcp`(op0@_, op1@_) => expand(op0, op1) 
  case `/dev/udp`(op0@_, op1@_) => expand(op0, op1) 
  case AnsiCQuoted(op0@_) => expand(op0) 
  case ArithmeticExpression(op0@_) => expand(op0) 
  case ArrayVariable(op0@_) => expand(op0) 
  case BashCliArgVariable(op0@_, op1@_) => expand(op0, op1) 
  case BashCliFlagArgVariable(op0@_, op1@_, op2@_) => expand(op0, op1, op2) 
  case BashCliOptArgVariable(op0@_, op1@_, op2@_) => expand(op0, op1, op2) 
  case BashCliVecArgVariable(op0@_, op1@_, op2@_) => expand(op0, op1, op2) 
  case BashVariable(op0@_, op1@_, op2@_, op3@_) => expand(op0, op1, op2, op3) 
  case CDo(op0@_) => expand(op0) 
  case CElif(op0@_) => expand(op0) 
  case CElse(op0@_) => expand(op0) 
  case CFileDescriptorIsOpenAndReferTerminal(op0@_, op1@_) => expand(op0, op1) 
  case CFor(op0@_) => expand(op0) 
  case CGroupIdBitSet(op0@_, op1@_) => expand(op0, op1) 
  case CIf(op0@_, op1@_) => expand(op0, op1) 
  case CIfIsFile(op0@_, op1@_) => expand(op0, op1) 
  case CIn(op0@_) => expand(op0) 
  case CIsBlock(op0@_, op1@_) => expand(op0, op1) 
  case CIsCharacter(op0@_, op1@_) => expand(op0, op1) 
  case CIsDirectory(op0@_, op1@_) => expand(op0, op1) 
  case CIsExecutable(op0@_, op1@_) => expand(op0, op1) 
  case CIsFile(op0@_, op1@_) => expand(op0, op1) 
  case CIsGreaterThanZero(op0@_, op1@_) => expand(op0, op1) 
  case CIsModifiedSinceLastRead(op0@_, op1@_) => expand(op0, op1) 
  case CIsNamedPipe(op0@_, op1@_) => expand(op0, op1) 
  case CIsOwnedByEffectiveGroupId(op0@_, op1@_) => expand(op0, op1) 
  case CIsOwnedByEffectiveUserId(op0@_, op1@_) => expand(op0, op1) 
  case CIsReadAble(op0@_, op1@_) => expand(op0, op1) 
  case CIsSocket(op0@_, op1@_) => expand(op0, op1) 
  case CIsSymbolLink(op0@_, op1@_) => expand(op0, op1) 
  case CIsSymbolicLink(op0@_, op1@_) => expand(op0, op1) 
  case CIsWritable(op0@_, op1@_) => expand(op0, op1) 
  case CStickyBitSet(op0@_, op1@_) => expand(op0, op1) 
  case CThen(op0@_) => expand(op0) 
  case CUntil(op0@_, op1@_) => expand(op0, op1) 
  case CUserIdBitSet(op0@_, op1@_) => expand(op0, op1) 
  case CWhile(op0@_, op1@_) => expand(op0, op1) 
  case CmdArgCtx(op0@_, op1@_) => expand(op0, op1) 
  case CmdArgs(op0@_) => expand(op0) 
  case CommandListBuilder(op0@_) => expand(op0) 
  case CommentLine(op0@_, op1@_) => expand(op0, op1) 
  case ConditionalBuilder(op0@_) => expand(op0) 
  case DebugValue(op0@_) => expand(op0) 
  case FileDescriptor(op0@_) => expand(op0) 
  case FileName(op0@_, op1@_) => expand(op0, op1) 
  case FilePath(op0@_, op1@_, op2@_) => expand(op0, op1, op2) 
  case FolderPath(op0@_, op1@_) => expand(op0, op1) 
  case HereDoc(op0@_, op1@_, op2@_) => expand(op0, op1, op2) 
  case HereString(op0@_, op1@_) => expand(op0, op1) 
  case IntVariable(op0@_) => expand(op0) 
  case LocalizationString(op0@_) => expand(op0) 
  case ParameterExpander(op0@_) => expand(op0) 
  case ParameterExpanderVariable(op0@_) => expand(op0) 
  case PipelineBuilder(op0@_) => expand(op0) 
  case RelFolderPath(op0@_) => expand(op0) 
  case RelPath(op0@_, op1@_) => expand(op0, op1) 
  case SheBang(op0@_) => expand(op0) 
  case SimpleCommand(op0@_, op1@_, op2@_, op3@_) => expand(op0, op1, op2, op3) 
  case SubShellVariable(op0@_) => expand(op0) 
  case TextVariable(op0@_) => expand(op0) 
  case a => a
}
